extern int printf(string str)
extern int puts(string str)

int gcd(int x, int y){
    int m
    int n
    int r
    m = x
    n = y
    while(n!=0){
        r = m-(n*m/n)
        m = n
        n = r
    }
    return m
}

int main(){
    printf("gcd(54, 18)=%d", gcd(54,18))
    puts("")
    return 0
}
